GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Mantle" "${PODS_ROOT}/Headers/Public/ORSSerialPort"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Mantle" -isystem "${PODS_ROOT}/Headers/Public/ORSSerialPort"
OTHER_LDFLAGS = $(inherited) -ObjC -l"Mantle" -l"ORSSerialPort" -framework "Foundation" -framework "IOKit"
PODS_ROOT = ${SRCROOT}/Pods